{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Telemetry Domain Contract",
  "description": "Auto-generated contract for Telemetry feature",
  "definitions": {
    "TelemetryEvent": {
      "properties": {
        "event_name": {
          "type": "string",
          "maxLength": 100,
          "minLength": 1,
          "title": "Event Name"
        },
        "properties": {
          "additionalProperties": true,
          "type": "object",
          "title": "Properties"
        }
      },
      "type": "object",
      "required": ["event_name"],
      "title": "TelemetryEvent",
      "description": "Telemetry event payload from the frontend."
