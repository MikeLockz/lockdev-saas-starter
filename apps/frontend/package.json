{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate:schema": "cd ../backend && .venv/bin/python scripts/generate_schema.py > openapi.json",
    "generate:types": "openapi-typescript ../backend/openapi.json -o src/lib/api-types.d.ts && openapi-zod-client ../backend/openapi.json -o src/lib/api-schemas.ts",
    "generate:models": "node scripts/generate-models.cjs",
    "check:any": "bash scripts/check-any.sh",
    "test": "vitest --run",
    "test:e2e": "playwright test"
  },
