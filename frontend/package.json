{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "test": "vitest run",
    "generate:types": "cd ../backend && uv run scripts/generate_schema.py && cd ../frontend && openapi-typescript ../backend/openapi.json -o src/lib/api-types.d.ts && openapi-zod-client ../backend/openapi.json -o src/lib/api-schemas.ts",
    "gen:api": "openapi-typescript http://localhost:8000/openapi.json -o src/lib/api-schema.d.ts",
    "lint": "biome check .",
    "format": "biome format --write .",
    "preview": "vite preview"
  },
  "dependencies": {
